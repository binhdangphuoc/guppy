on\" v-show=\"show == 2\">\n        <div class=\"rows\" v-for=\"(row, index) in numberRows\" :key=\"index\">\n          <input type=\"text\" v-model=\"rows[index]\">\n          <img :src=\"rows[index]\" alt=\"add images\">\n        </div>\n      </div>\n      <button @click=\"addRows\"><i class=\"fas fa-plus\"></i></button>\n      <button @click=\"subRows\"><i class=\"fas fa-minus\"></i></button>\n      <button @click=\"test\">test</button>\n  </div>\n</template>\n\n<script>\nexport default {\n    name: \"AddImagesVides\",\n    data() {\n      return {\n        rows: [],\n        numberRows: 1,\n        show: 1,\n        title: \"images\",\n      }\n    },\n    methods: {\n      test() {\n        console.log(this.rows);\n      },\n      changeShow(i, title) {\n        this.show = i;\n        this.title = title;\n      },\n      addRows() {\n        this.numberRows += 1;\n      },\n      subRows() {\n        console.log(this.rows[this.numberRows-1])\n        if(this.rows[this.numberRows-1] != \"\" && this.rows[this.numberRows-1] != undefined) {\n            if(confirm(\"Are you sure delete rows?\"))\n              {\n                if(this.numberRows > 0) this.numberRows -= 1;\n              }\n        } else {\n          if(this.numberRows > 0) this.numberRows -= 1;\n        }\n      }\n    },\n\n}\n</script>\n\n<style>\n  .add-images-videos {\n    display: flex;\n    flex-direction: column;\n    width: 100%;\n  }\n</style>"]}]}